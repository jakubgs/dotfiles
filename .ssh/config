Host github.com
    CanonicalizeHostname no
    PreferredAuthentications publickey
    PubkeyAuthentication yes
    ControlMaster auto
    ControlPersist 120

Host *.status.im *.develp.co
    CanonicalizeHostname no
    VerifyHostKeyDNS yes

Host 127.0.0.1
    UserKnownHostsFile /dev/null

Host ubuntuvm
    HostName 192.168.122.125

Host storagebox u288137.your-storagebox.de
    User u288137
    Port 23
    HostName u288137.your-storagebox.de
    IdentityFile ~/.ssh/restic


Host eve melchior caspair lilim balthasar leliel ramiel sachiel bardiel arael zeruel shamshel israfel iruel
    CanonicalizeHostname yes
    CanonicalDomains magi.vpn magi.lan
    CanonicalizeMaxDots 2

Host *
    Compression yes
    TCPKeepAlive yes
    ServerAliveInterval 5
    ServerAliveCountMax 60
    ForwardAgent yes
    SetEnv TERM=xterm
    # Resolve host domains automatically
    HashKnownHosts no
    CanonicalizeHostname yes
    CanonicalDomains status.im develp.co
    CanonicalizeMaxDots 3
    CanonicalizeFallbackLocal yes
    # Faster by using shared channel for all sessions for same host.
    ControlMaster auto
    ControlPath ~/.ssh/control/%r@%h:%p
    ControlPersist 1800
