def_key "insert"
    select_item

def_key "j"
    scroll_down 

def_key "k"
    scroll_up

def_key "h"
    seek_backward 

def_key "l"
    seek_forward 

def_key "L"
    next 

def_key "H"
    previous 

def_key "K"
    scroll_up_album 

def_key "J"
    scroll_down_album 

def_key "ctrl_k"
    move_selected_items_up

def_key "ctrl_j"
    move_selected_items_down 

def_key "y"
    show_lyrics 

def_key "0"
    toggle_single 

def_key "g"
    move_home 

def_key "G"
    move_end 

def_key "p"
    pause 

def_key "x"
    delete_playlist_items 

def_key "n"
    next_found_item

def_key "N"
    previous_found_item

def_key "P"
    toggle_display_mode 

def_key "+"
    show_clock

def_key "="
    volume_up

def_key "-"
    volume_down
