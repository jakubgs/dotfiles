[user]
	email = panswiata@gmail.com
	name = Jakub Soko≈Çowski
[core]
	editor = vim
[merge]
	tool = vimdiff
[alias]
    hist = log --graph
    co = checkout
    br = branch
    ps = push
    pl = pull
    sm = submodule
    c = commit
    s = status
    a = add 
    l  = log --decorate
    sl = log --graph --pretty=format:'%h : %s'
[branch "master"]
    remote = origin
    merge = refs/heads/master
[credential]
	helper = cache --timeout=1800
[color]
	ui = true
[push]
    default = current
[pull]
    default = current
